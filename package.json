{
  "name": "fishpi",
  "version": "0.1.3",
  "description": "A Package to use API of fishpi.",
  "type": "module",
  "types": "lib/index.d.ts",
  "main": "lib/index.cjs",
  "module": "lib/index.esm.js",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib/index*",
    "README.md"
  ],
  "scripts": {
    "cli": "node lib/cli.cjs",
    "start": "npm run build && npm run cli",
    "clear": "rimraf lib/*.map lib/*.d.ts lib/*.cjs",
    "clear:types": "rimraf lib/types",
    "format": "prettier --write src/",
    "link:cli": "node ./utils/cli-link.cjs",
    "prebuild": "npm run clear && npm run link:cli && npm run format && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir ./lib/types",
    "build": "rollup -c && npm run clear:types",
    "prepare": "npm run build"
  },
  "keywords": [
    "fishpi",
    "club",
    "api",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/FishPiOffical/fishpi.js/issues"
  },
  "homepage": "https://github.com/FishPiOffical/fishpi.js#readme",
  "author": {
    "name": "Hancel.Lin",
    "email": "imlinhanchao@foxmail.com",
    "url": "https://github.com/imlinhanchao"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "js-md5": "^0.7.3",
    "reconnecting-websocket": "^4.4.0",
    "tslib": "^2.8.1",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/blessed": "^0.1.25",
    "@types/js-md5": "^0.8.0",
    "@types/node": "^24.3.0",
    "@types/node-fetch": "^2.6.13",
    "@types/ws": "^8.18.1",
    "blessed": "^0.1.81",
    "commander": "^14.0.0",
    "inquirer": "^12.9.4",
    "node-fetch": "^2.7.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.48.1",
    "rollup-plugin-dts": "^6.2.3",
    "typescript": "^5.9.2"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
